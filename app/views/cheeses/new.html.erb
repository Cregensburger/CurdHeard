<%= form_for :cheese, url: cheeses_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :'country of Origin' %><br>
    <%= f.text_area :country %>
  </p>

   <p>
    <%= f.label :type %><br>
    <%= f.text_area :kind %>
  </p>

   <p>
    <%= f.label :picture %><br>
    <%= f.text_area :picture %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>